{
  "expo": {
    "name": "Reparo",
    "slug": "reparo",
    "version": "1.0.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reparo.app"
    },
    "android": {
      "package": "com.reparo.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "NOTIFICATIONS"
      ],
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#3b82f6"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#3b82f6",
      "androidMode": "default",
      "androidCollapsedTitle": "Reparo",
      "iosDisplayInForeground": true
    },
    "extra": {
      "eas": {
        "projectId": "db3a5438-564d-414d-83bb-9635942f5603"
      }
    },
    "owner": "abhiiii.tiwari"
  }
}
